<?xml version="1.0" encoding="utf-8"?>
<layout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:id="@+id/mycommunitylinearLayout"
        android:paddingStart="@dimen/dp_16"
        android:paddingTop="@dimen/dp_4"
        android:paddingEnd="@dimen/dp_16"
        android:paddingBottom="@dimen/dp_4">


        <LinearLayout
            android:id="@+id/linearcommunitylayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_marginBottom="@dimen/dp_8"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/tvChallengesNameMyCommunity"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/din_alternate_bold"
                android:gravity="start"
                android:text="Delhi Joggers"
                android:textColor="@color/black_btn"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/memberssMyCommunity"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/din_alter_nate_bold"
                android:gravity="end"
                android:text="1000"
                android:textColor="@color/light_blue"
                android:textSize="@dimen/sp_14" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/bottom_gray" />


    </LinearLayout>
</layout>
